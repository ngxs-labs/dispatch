{
    "$schema": "../node_modules/ng-packagr/package.schema.json",
    "name": "@ngxs-labs/dispatch-decorator",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ngxs-labs/dispatch-decorator.git"
    },
    "license": "MIT",
    "homepage": "https://github.com/ngxs-labs/dispatch-decorator#readme",
    "bugs": {
        "url": "https://github.com/ngxs-labs/dispatch-decorator/issues"
    },
    "keywords": [
        "ngxs",
        "redux",
        "store"
    ],
    "sideEffects": true,
    "peerDependencies": {
        "@angular/core": "^7.0.0",
        "@ngxs/store": ">=3.3.0"
    },
    "ngPackage": {
        "lib": {
            "flatModuleFile": "ngxs-labs-dispatch",
            "entryFile": "index.ts",
            "umdModuleIds": {
                "@ngxs/store": "ngxs-store"
            }
        },
        "dest": "../dist/dispatch"
