{
  "$schema": "../node_modules/ng-packagr/package.schema.json",
  "name": "@ngxs-labs/dispatch-decorator",
  "version": "2.1.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngxs-labs/dispatch-decorator.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/ngxs-labs/dispatch-decorator#readme",
  "bugs": {
    "url": "https://github.com/ngxs-labs/dispatch-decorator/issues"
  },
  "keywords": [
    "ngxs",
    "redux",
    "store"
  ],
  "sideEffects": true,
  "peerDependencies": {
    "@angular/core": ">=12.0.0 <13.0.0",
    "@ngxs/store": ">=3.7.2",
    "rxjs": ">=7.1.0"
  },
  "ngPackage": {
    "lib": {
      "flatModuleFile": "ngxs-labs-dispatch",
      "entryFile": "index.ts",
      "umdModuleIds": {
        "@ngxs/store": "ngxs-store"
      }
    },
    "dest": "../dist/dispatch"
